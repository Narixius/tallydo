{
  "name": "tallydo",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://narixius.github.io/tallydo",
  "dependencies": {
    "dayjs": "^1.9.6",
    "dotenv": "^8.2.0",
    "heroicons-react": "^1.3.0",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-dates": "^21.8.0",
    "react-dom": "^17.0.1",
    "react-flip-move": "^3.0.4",
    "react-redux": "^7.2.2",
    "react-scripts": "3.4.4",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "tailwindcss": "^1.9.6",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-color": "^3.0.4",
    "@types/react-dates": "^21.8.0",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "@types/react-transition-group": "^4.4.0",
    "@types/redux": "^3.6.0",
    "autoprefixer": "^10.0.2",
    "create-react-app": "^4.0.0",
    "cross-env": "^7.0.2",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.7",
    "postcss-cli": ">=8.0",
    "selenium-webdriver": "^4.0.0-alpha.8",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "dev": "yarn tailwind:compile && react-scripts start",
    "start": "react-scripts start",
    "build": "cross-env NODE_ENV=production yarn tailwind:compile && react-scripts build",
    "eject": "react-scripts eject",
    "tailwind:compile": "postcss src/tailwind.css -o public/assets/tailwind.css",
    "tailwind:watch": "postcss src/tailwind.css -o public/assets/tailwind.css --watch",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "run-p test:*",
    "test:server": "http-server -p 8888 ./build > /dev/null 2>&1 &",
    "test:run": "node ./tests/AddTodo.test.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
